install.packages("pacman") #es un paquete que llama a otros paquetes y si 

library("pacman") #esta función llama al paquete instalado

p_load("ggplot2", "dplyr", "vroom")

Datos_curva <- vroom(file="https://raw.githubusercontent.com/ManuelLaraMVZ/resultados_PCR_practica/refs/heads/main/Amplif_grupo1_17022025.csv")
head(Datos_curva) 

Curva_1 <- Datos_curva %>% #el nuevo objeto va a tener uan asignación

  select("Cycle", "F1")
 Curva_1 

Grafica_curva <- ggplot(Curva_1,
                         mapping = aes(x=Cycle,
                                       y=F1))+
   geom_point()+
  geom_line(color="blue",
            linewidth=1)
Grafica_curva

Grafica_curva_2 <- Grafica_curva+
  labs(x="ciclos",
       y="Fluorescencia",
       title = "creando por: Samantha Carrillo",
       subtitle = "Grafica F1",
       caption = "Ejercicio 3")+
  theme_classic()
Grafica_curva_2





